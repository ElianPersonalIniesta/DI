<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VistasyEso.Resources.Styles.Dictionary1">
    <Style TargetType="VerticalStackLayout">
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={DynamicResource BlueMenu}, Dark={DynamicResource BlueMenu}}"/>
        
    </Style>
    <Style TargetType="ShellContent">
        <Setter Property="Shell.BackgroundColor" Value="{AppThemeBinding Light={DynamicResource MidnightBlue}, Dark={DynamicResource MidnightBlue}}"/>
    </Style>

</ResourceDictionary>